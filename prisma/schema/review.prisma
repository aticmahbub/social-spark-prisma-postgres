// REVIEWS
model Review {
  id         String    @id @default(uuid())
  rating     Int
  comment    String?

  reviewer   User     @relation("UserReviews", fields: [reviewerId], references: [id])
  reviewerId String

  event      Event    @relation(fields: [eventId], references: [id])
  eventId    String

  createdAt  DateTime  @default(now())
}
